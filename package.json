{
  "name": "aws-react-icons",
  "version": "3.2.0",
  "description": "AWS React Icons, a React library for AWS icons based on the official AWS icon set (SVG) and built with TypeScript.",
  "keywords": [
    "aws",
    "react",
    "icons",
    "typescript",
    "svg",
    "jsx",
    "tsx"
  ],
  "license": "MIT",
  "author": {
    "name": "Mohammad Abu Mattar",
    "email": "mohammad.khaled@outlook.com",
    "url": "https://mkabumattar.github.io/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MKAbuMattar/aws-react-icons.git"
  },
  "bugs": {
    "url": "https://github.com/MKAbuMattar/aws-react-icons/issues"
  },
  "homepage": "https://aws-icon.vercel.app/",
  "scripts": {
    "lint": "biome check .",
    "lint:fix": "biome check --fix .",
    "fmt": "biome format .",
    "fmt:fix": "biome format --write .",
    "clean:aws-icons": "rm -rf ./aws-icons",
    "clean:aws-icons:macosx": "rm -rf ./aws-icons/__MACOSX",
    "clean:icons": "rm -rf ./icons",
    "clean:svg": "rm -rf ./svg",
    "clean:build": "rm -rf ./build",
    "clean:lib": "rm -rf ./lib",
    "download:icons": "pnpm clean:aws-icons && pnpm clean:icons && pnpm clean:svg && tsx ./src/download.ts && pnpm clean:aws-icons:macosx",
    "cleanup:icons": "tsx ./src/cleanup.ts",
    "svg:optmize": "svgo --config svgo.config.cjs -f svg/ -o icons/",
    "build:script:jsx": "pnpm clean:build && tsx ./src/jsx.script.ts",
    "build:lib": "pnpm clean:lib && babel build --out-dir lib --copy-files",
    "generate:data": "tsx ./src/generate.data.ts",
    "build": "pnpm download:icons && pnpm cleanup:icons && pnpm svg:optmize && pnpm build:script:jsx && pnpm build:lib && pnpm generate:data",
    "publish:lib:latest": "npm publish",
    "publish:lib:beta": "npm publish --tag beta",
    "publish:lib:canary": "npm publish --tag canary"
  },
  "peerDependencies": {
    "react": "*"
  },
  "devDependencies": {
    "@babel/cli": "7.28.3",
    "@babel/core": "7.28.4",
    "@babel/plugin-syntax-jsx": "7.27.1",
    "@babel/plugin-transform-parameters": "7.27.7",
    "@babel/preset-react": "7.27.1",
    "@biomejs/biome": "2.2.4",
    "@types/adm-zip": "0.5.7",
    "@types/eslint": "9.6.1",
    "@types/jsdom": "21.1.7",
    "@types/node": "24.4.0",
    "@types/react": "19.1.13",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "8.43.0",
    "@typescript-eslint/parser": "8.43.0",
    "adm-zip": "0.5.16",
    "babel-preset-minify": "0.5.2",
    "eslint": "9.35.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-unused-imports": "4.2.0",
    "jsdom": "27.0.0",
    "prettier": "3.6.2",
    "react": "19.1.1",
    "recursive-directory": "1.2.3",
    "svg-to-jsx": "1.0.4",
    "svgo": "4.0.0",
    "tsx": "4.20.5",
    "typescript": "5.9.2",
    "uuid": "13.0.0"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./lib/index.js",
      "types": "./index.d.ts"
    },
    "./lib/icons/*": {
      "require": "./lib/icons/*.js",
      "import": "./lib/icons/*.js",
      "types": "./lib/icons/*.d.ts"
    },
    "./icons/*": {
      "require": "./lib/icons/*.js",
      "import": "./lib/icons/*.js",
      "types": "./lib/icons/*.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "/lib",
    "/index.d.ts"
  ],
  "support": {
    "backing": {
      "npm-funding": true
    }
  }
}
